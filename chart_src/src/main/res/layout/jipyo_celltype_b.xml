<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:id="@+id/fctb_AbsoluteLayout01" 
	android:layout_width="fill_parent" android:orientation="horizontal"
	android:layout_height="40dp"
	android:background="#ffffff" android:weightSum="1"
	android:gravity="center_vertical"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<FrameLayout
		android:id="@+id/fctb_TextViewA01_back"
		android:layout_width="33dp"
		android:layout_height="match_parent">
		<CheckBox android:text=""
					android:id="@+id/usf_CheckBoxB01"
					android:checked="false" android:button="@drawable/checkbox_change"
					android:focusable="false" android:layout_marginLeft="12dp" android:layout_width="26dp" android:layout_height="match_parent">
				</CheckBox>
	</FrameLayout>
	<TextView android:text="지표이름"
		android:id="@+id/fctb_TextViewA01" android:layout_weight="1"
		 android:textColor="#000000" android:gravity="center_vertical"
		 android:layout_height="match_parent" android:layout_width="0dp" android:layout_marginLeft="8dp" android:textSize="14dp">
		</TextView>

	<TextView android:background="@drawable/btn_duplicatedindicator_plus_change"
		android:id="@+id/tv_add_or_del_jipyo" android:textColor="#3a4d70" android:gravity="center"
		android:layout_width="46dp" android:layout_height="40dp" android:textSize="12dp">
		</TextView>

	<TextView   android:background="@drawable/btn_showdetail_change"
		android:id="@+id/tv_setjipyo" android:textColor="#000000" android:gravity="center" 
		android:layout_width="46dp" android:layout_height="40dp"  android:textSize="12dp" android:layout_marginRight="2dp">
		</TextView>
</LinearLayout>
