<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	>
	<LinearLayout android:id="@+id/LinearLayout01"
		android:layout_width="fill_parent" 
		android:layout_height="54px"
		android:paddingTop="5px" 
		android:orientation="horizontal">
		<TextView android:id="@+id/frameatitle" 
			android:layout_width="150dp" 
			android:layout_height="wrap_content" 
			android:text="" android:width="200dp" 
			android:textStyle="bold" android:gravity="center" android:layout_weight="1">
		</TextView>
		<Button android:id="@+id/frameaBtnFunction"
			android:layout_height="28dp" 
			android:layout_width="47dp"
			android:layout_marginLeft="5dp"
			android:layout_marginTop="2dp" 
			android:layout_marginRight="10dip">
		</Button>		
	</LinearLayout>

	<ListView android:layout_height="wrap_content" android:id="@+id/jipyolist" android:layout_width="match_parent" android:cacheColorHint="#00000000"></ListView>
</LinearLayout>
