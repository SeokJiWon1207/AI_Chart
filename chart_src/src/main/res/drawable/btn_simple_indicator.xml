            android:shape="rectangle">

            <solid android:color="#f36f7782" />

            <corners
                android:bottomLeftRadius="0dp"
                android:bottomRightRadius="4dp"
                android:topLeftRadius="0dp"
                android:topRightRadius="4dp" />

        </shape>
    </item>
</layer-list>